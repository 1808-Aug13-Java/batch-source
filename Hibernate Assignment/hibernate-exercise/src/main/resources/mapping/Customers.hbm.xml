<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.revature.models.Customers">
		<id name="id" column="CUSTOMER_ID">
	<generator class="increment"></generator>
</id>
	<property name="name" type ="org.hibernate.type.StringType"	column="CUSTOMER_NAME"></property>
	<property name="phoneNumber" column="PHONE_NUMBER"></property>

	<!-- name in many to one is the attribute -->
</class>
</hibernate-mapping>
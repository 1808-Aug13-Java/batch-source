COMMIT;

SELECT STREET STREET1, CITY CITY1, STATE STATE1
FROM LOCATION;

SAVEPOINT SP1;

UPDATE LOCATION
SET CITY = 'KANSAS CITY';

SELECT STREET STREET2, CITY CITY2, STATE STATE2
FROM LOCATION;

ROLLBACK TO SP1;

UPDATE LOCATION
SET CITY = 'OKLAHOMA CITY'
WHERE LOCATION_ID = 3;

SELECT STREET STREET3, CITY CITY3, STATE STATE3
FROM LOCATION;

ROLLBACK;
COMMIT;
SELECT STREET STREET4, CITY CITY4, STATE STATE4
FROM LOCATION;






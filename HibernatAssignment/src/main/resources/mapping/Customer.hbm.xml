<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.revature.models.Customer" table="CUSTOMER">
		<id name="id" column="CUST_ID">
			<generator class="increment"></generator>
		</id>
		
		<property name="name" type="org.hibernate.type.StringType" column="CUST_NAME"></property>
		
		<property name="phoneNumber" type="org.hibernate.type.StringType" column="CUST_PHONE"></property>
	</class>
</hibernate-mapping>



